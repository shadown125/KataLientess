{
  "name": "katalientess",
  "description": "KataLientesS App",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "stylebuild": "npm-run-all --parallel copy:* build:*",
    "build:styles": "npm-run-all sass:build postcss:build",
    "build:images": "imagemin src/pics/*.{jpg,png} --out-dir=public",
    "copy:fonts": "copyfiles src/fonts/**/* public --flat",
    "watch": "npm-run-all --parallel watch:*",
    "watch:styles": "npm-run-all --parallel sass:watch postcss:watch",
    "sass:build": "sass src/styles:public --no-source-map",
    "sass:watch": "sass src/styles:public --no-source-map --watch",
    "postcss:build": "postcss public/*.css --replace --env production",
    "postcss:watch": "postcss public/*.css --watch --dir public --poll=1000",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "> 0.5%",
    "not ie <= 11"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "formik": "^2.2.9",
    "mongodb": "^4.2.2",
    "next": "12.0.4",
    "next-auth": "^4.0.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.1.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "autoprefixer": "^10.3.1",
    "babel-jest": "^27.4.5",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.0.13",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "eslint-plugin-testing-library": "^5.0.1",
    "imagemin": "^7.0.1",
    "imagemin-cli": "^6.0.0",
    "jest": "^27.4.5",
    "msw": "^0.36.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-cli": "^8.3.1",
    "postcss-csso": "^5.0.0",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.45.0",
    "stylelint": "^13.13.1",
    "svgo": "^2.4.0"
  }
}
