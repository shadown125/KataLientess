{
  "name": "katalientess",
  "description": "KataLientesS App",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "stylebuild": "npm-run-all --parallel copy:* build:*",
    "build:styles": "npm-run-all sass:build postcss:build",
    "build:images": "imagemin src/pics/*.{jpg,png} --out-dir=public",
    "copy:fonts": "copyfiles src/fonts/**/* public --flat",
    "watch": "npm-run-all --parallel watch:*",
    "watch:styles": "npm-run-all --parallel sass:watch postcss:watch",
    "sass:build": "sass src/styles:public --no-source-map",
    "sass:watch": "sass src/styles:src/build --no-source-map --watch",
    "postcss:build": "postcss public/*.css --replace --env production",
    "postcss:watch": "postcss src/build/*.css --watch --dir public",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "> 0.5%",
    "not ie <= 11"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "formik": "^2.2.9",
    "mongodb": "^4.4.1",
    "next": "^12.1.0",
    "next-auth": "^4.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.2.2",
    "whatwg-fetch": "^3.6.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^17.0.41",
    "@types/react-test-renderer": "^17.0.1",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^27.5.1",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.1.4",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "eslint-plugin-testing-library": "^5.1.0",
    "imagemin": "^7.0.1",
    "imagemin-cli": "^7.0.0",
    "jest": "^27.5.1",
    "msw": "^0.36.8",
    "next-secure-headers": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-cli": "^8.3.1",
    "postcss-csso": "^5.0.1",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.49.9",
    "stylelint": "^14.6.0",
    "svgo": "^2.8.0",
    "webpack-subresource-integrity": "^5.1.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
